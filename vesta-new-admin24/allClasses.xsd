        <xs:element name="//消息创建时间messagecreatetime" type="xs:dateTime" />
        <xs:element name="//消息目标网址messagetargeturl" type="xs:string" />
        <xs:element name="//消息发件人messagesendername" type="xs:string" />
        <xs:element name="//消息类型员工端：公区，户内，通知，会议，投诉；业主端：分类一，分类二messagetype" type="xs:string" />
