<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:complexType name="gathering" >
      <xs:sequence>
        <xs:element name="//编号。id" type="xs:string" />
        <xs:element name="//活动名称。name" type="xs:string" />
        <xs:element name="//大会简介。summary" type="xs:string" />
        <xs:element name="//详细说明。detail" type="xs:string" />
        <xs:element name="//主办方。sponsor" type="xs:string" />
        <xs:element name="//活动图片。image" type="xs:string" />
        <xs:element name="//开始时间。starttime" type="xs:anyType" />
        <xs:element name="//截止时间。endtime" type="xs:anyType" />
        <xs:element name="//举办地点。address" type="xs:string" />
        <xs:element name="//报名截止。enrolltime" type="xs:anyType" />
